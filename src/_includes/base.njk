  <body>
  {% block navbar %}
    <nav class="navbar navbar-expand-lg site-navigation">
      <div class="container">
        <a class="navbar-brand" href="/">
          <img src="{{nav.navLogo}}" alt="nav-logo" />
        </a>
        <button
          class="navbar-toggler collapsed"
          type="button"
          data-toggle="collapse"
          data-target="#sitenavbar"
        >
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      
        <div class="collapse navbar-collapse" id="sitenavbar">
          <ul class="navbar-nav ml-auto main-nav">
            {% for navLink in nav.navLinks %}
            <li class="nav-item">
              {% if navLink == "home" %}
                <a class="nav-link" href="/#">{{navLink | capitalize}}</a>
              {% elif navLink == "contact us" %}
                <a class="nav-link" href="/#contact">{{navLink | capitalize}}</a>
              {% elif navLink == "about us" %}
                <a class="nav-link" href="/#about">{{navLink | capitalize}}</a>
              {% elif navLink == "brands" %}
                <a class="nav-link" href="/#brands">{{navLink | capitalize}}</a>
              {% else %}
                <a class="nav-link" href="/{{navLink}}">{{navLink | capitalize}}</a> 
              {% endif %} 
            </li>
            {% endfor %}
          </ul>
        </div>
      </div>
    </nav>
  {% endblock %}

  {% block content %}
    {{content | safe}}
  {% endblock %}

  {% block whatsapp%}
  <a href="https://wa.me/94727866579?text=I%20would%20like%20to%20inquire%20about%20a%20brand%20under%20Finco%20Trading." class="float" target="_blank">
    <img alt="whatsapp" src="/assets/images/social_icons/whatsapp.svg" width="45" class="my-float">
  </a>
  {% endblock %}

  {% block footer %}
    <footer class="site-footer">
      <div class="container" id="contact">
        <div class="row">
          <div class="col-12">
            <div class="site-footer-logo">
              <a href="/">
                <img src="{{footer.footerLogo}}" alt="logo-footer" style="max-width: {{footer.footerLogoWidth}};">
              </a>
            </div>
          </div>
          <div class="col-lg-3 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">Contact Info</h5>
              <p class="site-footer-widget-description">
              {% for contactInfo in footer.contactsInfo %}
                <a href="{{contactInfo.link}}">{{contactInfo.text}}</a>
                <br>
              {% endfor %}
            </div>
          </div>
          <div class="col-lg-2 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">{{footer.siteMapHeading}}</h5>
              <ul class="site-footer-widget-links">
                {% for siteMap in footer.siteMap %}
                 <li>
                  <a href="/{{siteMap.link}}">{{siteMap.text}}</a>
                 </li> 
                {% endfor %}
              </ul>
            </div>
          </div>
          <div class="col-lg-2 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">{{footer.socialHeading}}</h5>
              <ul class="site-footer-widget-links">
                {% for socialMedia in footer.socialMedia %}
                <li><a href="/{{socialMedia.link}}">{{socialMedia.text}}</a></li>
                {% endfor %}
              </ul>
            </div>
          </div>
          <div class="col-lg-3 col-md-6">
            <div class="site-footer-widget">
              <h5 class="site-footer-widget-title">{{footer.brandHeading}}</h5>
              <p class="site-footer-widget-description">
              {% for brandMotto in footer.brandMotto %}
                - {{brandMotto}}
                <br>
              {% endfor %}
              </p>
            </div>
          </div>
          <div class="col-lg-2 col-12">
            <a href="#top" class="site-footer-widget-top">
              <img src="{{footer.back2topIcon}}" alt="back-to-top">
              <p>
                {{footer.customerComment}}
              </p>
            </a>
          </div>
        </div>
      </div>
    </footer>
  {% endblock %}
